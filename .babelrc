{
  "presets": [
    ["@babel/env", { "targets": "> 0.25%, not dead" }],
    "@babel/react"
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "corejs": 2
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@component": "./src/component",
          "@container": "./src/container",
          "@src": "./src",
          "@screen": "./src/screen",
          "@service": "./src/service",
          "@styles": "./src/styles"
        }
      }
    ]
  ]
}
